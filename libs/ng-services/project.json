{
  "projectType": "library",
  "sourceRoot": "libs/ng-services/src",
  "prefix": "trucks",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ng-services"],
      "options": {
        "jestConfig": "libs/ng-services/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ng-services/**/*.ts",
          "libs/ng-services/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
